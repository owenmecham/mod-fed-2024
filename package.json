{
  "name": "ng-fake-coin",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:shell",
    "start:shell": "nx serve shell  -o --port 5001",
    "start:mfe1": "nx serve mfe1 --port 3000",
    "start:mfe2": "nx serve mfe2 --port 3001",
    "start:all": "concurrently \"npm run start:shell\" \"npm run start:mfe1\" ",
    "build:shell": "nx build shell --configuration production",
    "build:mfe1": "nx build mfe1 --configuration production",
    "build:mfe2": "nx build mfe2 --configuration production",
    "serve:dist": "concurrently \"serve dist/shell -l 5001 -s\" \"serve dist/mfe1 -l 3000 -s\" ",
    "build": "npm run build:shell && npm run build:mfe1",
    "test": "nx test",
    "lint": "nx lint",
    "e2e": "nx e2e",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
